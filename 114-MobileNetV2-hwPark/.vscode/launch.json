            "request": "launch",
            "program": "${workspaceFolder}/build/114-MobileNetV2-hwPark",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
